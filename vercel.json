{
  "version": 2,
  "name": "salarycheck-us",
  "alias": ["salarycheck.us", "www.salarycheck.us"],

  "buildCommand": null,
  "outputDirectory": "./",
  "installCommand": null,

  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
    {
      "source": "/wage-calculator",
      "destination": "/wage-calculator.html"
    },
    {
      "source": "/salary-comparison-tool",
      "destination": "/salary-comparison-tool.html"
    },
    {
      "source": "/salary-checker/tech-industry",
      "destination": "/salary-checker/tech-industry.html"
    },
    {
      "source": "/salary-checker/healthcare",
      "destination": "/salary-checker/healthcare.html"
    },
    {
      "source": "/salary-checker/finance",
      "destination": "/salary-checker/finance.html"
    },
    {
      "source": "/salary-checker/engineering",
      "destination": "/salary-checker/engineering.html"
    }
  ],

  "redirects": [
    {
      "source": "/wp-admin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/wordpress/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/wp-admin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/wp-includes/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/wp-content/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/.aws/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/_profiler/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/phpinfo(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/.env(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/config/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/admin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/administrator/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/phpmyadmin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/mysql/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*).sql",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*).bak",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*).backup",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/xmlrpc.php",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/xmlrpc.php",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/cmd_sco",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/license.txt",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/wp-includes/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    }
  ],

  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-Permitted-Cross-Domain-Policies",
          "value": "none"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://pagead2.googlesyndication.com https://www.googletagmanager.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https:; connect-src 'self' https://api.bls.gov https://www.google-analytics.com; frame-src https://googleads.g.doubleclick.net;"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

  "trailingSlash": false,

  "cleanUrls": true,

  "public": true
}
