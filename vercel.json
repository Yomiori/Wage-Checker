{
  "version": 2,
  "name": "salarycheck-us",
  "alias": ["salarycheck.us", "www.salarycheck.us"],

  "redirects": [
    {
      "source": "/wp-admin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/wordpress/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/wp-admin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/wp-includes/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/wp-content/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/.aws/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/_profiler/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/phpinfo(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/.env(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/config/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/admin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/administrator/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/phpmyadmin/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/mysql/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*).sql",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*).bak",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*).backup",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/xmlrpc.php",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/xmlrpc.php",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/cmd_sco",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/(.*)/license.txt",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/wp-includes/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    }
  ],

  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-Permitted-Cross-Domain-Policies",
          "value": "none"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://pagead2.googlesyndication.com https://www.googletagmanager.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https:; connect-src 'self' https://api.bls.gov https://www.google-analytics.com; frame-src https://googleads.g.doubleclick.net;"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

  "trailingSlash": false,

  "cleanUrls": true,

  "public": true
}
