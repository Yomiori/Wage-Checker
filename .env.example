# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Bureau of Labor Statistics API
BLS_API_KEY=your_bls_api_key_here

# Google AdSense Configuration
GOOGLE_ADSENSE_CLIENT=ca-pub-your_publisher_id_here
ADSENSE_HEADER_SLOT=your_header_ad_slot_id
ADSENSE_SIDEBAR_SLOT=your_sidebar_ad_slot_id
ADSENSE_BOTTOM_SLOT=your_bottom_ad_slot_id

# Google Analytics (optional)
GA_MEASUREMENT_ID=G-your_measurement_id

# Site Configuration
SITE_URL=https://your-domain.com
SITE_NAME=Am I Underpaid?

# Feature Flags
ENABLE_MOCK_DATA=false
ENABLE_ADS=true
ENABLE_ANALYTICS=true
